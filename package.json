{
  "name": "uncgbify",
  "author": "Mark Riedesel <mark@klowner.com>",
  "version": "1.0.0",
  "description": "Revert XCode PNG compression (CgBI)",
  "main": "dist-cjs/index.js",
  "module": "dist-es/index.js",
  "types": "dist-types/index.d.ts",
  "files": [
    "dist-cjs",
    "dist-types",
    "dist-es"
  ],
  "scripts": {
    "build": "concurrently 'yarn:build:es' 'yarn:build:cjs' 'yarn:build:types'",
    "build:es": "tsc -p tsconfig.es.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "check": "tsc --noEmit -p tsconfig.json",
    "watch": "tsc --noEmit --watch -p tsconfig.json",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/parser": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/types": "^7.20.5",
    "@types/jest": "^29.2.4",
    "@zip.js/zip.js": "^2.6.60",
    "concurrently": "^7.6.0",
    "crc-32": "^1.2.2",
    "jest": "^29.3.1",
    "pako": "^2.1.0",
    "ts-jest": "^29.0.3",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/pako": "^2.0.0"
  }
}
